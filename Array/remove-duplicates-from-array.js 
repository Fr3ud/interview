/**
 * Remove duplicates from unsorted array using Set data structure
 * 
 * The Set object lets you store unique values of any type,
 * whether primitive values or object references.
 */

const array = ['Alexander', 'ğŸ˜»', 'Maria', 'ğŸ˜»', 'Pavel', 'ğŸ˜»'];

const uniqArray1 = [...new Set(array)];
const uniqArray2 = Array.from(new Set(array));

console.log(uniqArray1); // ["Alexander", "ğŸ˜»", "Maria", "Pavel"]
console.log(uniqArray2); // ["Alexander", "ğŸ˜»", "Maria", "Pavel"]